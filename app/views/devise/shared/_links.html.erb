<%- if controller_name != 'sessions' %>
  <%= link_to "conectarme", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Inscribirme", new_registration_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Se me ha olvidado mi contrasena", new_password_path(resource_name) %><br />
<% end %>


